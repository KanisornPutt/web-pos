{"ast":null,"code":"import { computed, onMounted, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport ItemCard from \"@/components/dashboard/ItemCard.vue\";\nimport CartVue from \"@/components/dashboard/Cart.vue\";\nexport default {\n  components: {\n    ItemCard,\n    CartVue\n  },\n  setup() {\n    const store = useStore();\n    const user = computed(() => store.getters[\"auth/user\"]);\n    const showName = computed(() => store.getters[\"auth/showName\"]);\n    const storeId = computed(() => store.getters[\"auth/storeId\"]);\n    const storeData = computed(() => store.getters[\"auth/storeData\"]);\n    const isLinkedToStore = computed(() => store.getters[\"auth/storeData\"]);\n    const cart = computed(() => store.state.cart.cart);\n    onMounted(() => {});\n    return {\n      user,\n      cart,\n      storeId,\n      showName,\n      isLinkedToStore,\n      storeData\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","ref","useStore","ItemCard","CartVue","components","setup","store","user","getters","showName","storeId","storeData","isLinkedToStore","cart","state"],"sources":["/Users/shane/Desktop/isc project/frontend/web-pos 3.0/src/views/DashboardView.vue"],"sourcesContent":["<template>\n  <div v-if=\"user\">\n    <div class=\"row\">\n      <div class=\"container col-12 col-lg-9 col-xl-9\">\n        <div class=\"row mt-3\">\n          <div class=\"col-6\">\n            <h4 class=\"mx-5 my-2\">{{ showName }}'s Dashboard</h4>\n          </div>\n          <div class=\"col-6 text-end\">\n            <h4 v-if=\"isLinkedToStore\" class=\"mx-5 my-2\">{{ storeData.name }} : {{user.role}}</h4>\n          </div>\n        </div>\n\n        <hr class=\"mx-4\" />\n\n        <div v-if=\"!isLinkedToStore\" class=\"container\">\n          <div\n            class=\"row justify-content-center align-items-center\"\n            style=\"height: 50vh\"\n          >\n            <div class=\"col-6 col-md-4 text-center\">\n              <h4>No Store Found</h4>\n              <a class=\"btn btn-lg btn-orange text-light\" href=\"/storeSetup\"\n                >Link to One Now</a\n              >\n            </div>\n          </div>\n        </div>\n\n        <!-- For User's with Store id -->\n        <dir v-if=\"isLinkedToStore\">\n          <div class=\"container-fluid\">\n            <div\n              class=\"d-flex justify-content-between align-items-center mx-4 my-1\"\n            >\n              <h4 class=\"fw-light\">All Items</h4>\n              <form class=\"d-flex\" role=\"search\">\n                <input\n                  class=\"form-control me-2\"\n                  type=\"search\"\n                  placeholder=\"Search\"\n                  aria-label=\"Search\"\n                />\n                <button class=\"btn btn-outline-success\" type=\"submit\">\n                  Search\n                </button>\n              </form>\n            </div>\n          </div>\n\n          <hr class=\"me-5\" />\n\n\n          <!-- Products -->\n\n          \n\n\n          <!-- If store have products -->\n          <div class=\"row mb-5 mx-1\">\n            <div\n              class=\"col-6 col-sm-3 col-md-3 col-lg-3 col-xl-2 mb-4\"\n              v-for=\"item in user.items\"\n              :key=\"item.name\"\n            >\n              <ItemCard :item=\"item\" />\n            </div>\n          </div>\n        </dir>\n      </div>\n      <div\n        v-if=\"isLinkedToStore\"\n        class=\"col-lg-3 col-xl-3 bg-body-tertiary cart\"\n      >\n        <CartVue />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed, onMounted, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport ItemCard from \"@/components/dashboard/ItemCard.vue\";\nimport CartVue from \"@/components/dashboard/Cart.vue\";\n\nexport default {\n  components: { ItemCard, CartVue },\n  setup() {\n    const store = useStore();\n    const user = computed(() => store.getters[\"auth/user\"]);\n    const showName = computed(() => store.getters[\"auth/showName\"]);\n    const storeId = computed(() => store.getters[\"auth/storeId\"]);\n    const storeData = computed(() => store.getters[\"auth/storeData\"]);\n    const isLinkedToStore = computed(\n      () => store.getters[\"auth/storeData\"]\n    );\n    const cart = computed(() => store.state.cart.cart);\n\n    onMounted(() => {\n    });\n\n    return { user, cart, storeId, showName, isLinkedToStore, storeData };\n  },\n};\n</script>\n\n<style scoped>\n.cart {\n  /* Set default height */\n  height: 100rem; /* Height from lg to xxl */\n  position: relative;\n\n  /* Media query for sm to md */\n  @media (max-width: 992px) {\n    height: 0vh; /* Height at xl to md */\n    position: fixed;\n  }\n}\n</style>\n"],"mappings":"AAiFA,SAASA,QAAQ,EAAEC,SAAS,EAAEC,GAAE,QAAS,KAAK;AAC9C,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,QAAO,MAAO,qCAAqC;AAC1D,OAAOC,OAAM,MAAO,iCAAiC;AAErD,eAAe;EACbC,UAAU,EAAE;IAAEF,QAAQ;IAAEC;EAAQ,CAAC;EACjCE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC,CAAC;IACxB,MAAMM,IAAG,GAAIT,QAAQ,CAAC,MAAMQ,KAAK,CAACE,OAAO,CAAC,WAAW,CAAC,CAAC;IACvD,MAAMC,QAAO,GAAIX,QAAQ,CAAC,MAAMQ,KAAK,CAACE,OAAO,CAAC,eAAe,CAAC,CAAC;IAC/D,MAAME,OAAM,GAAIZ,QAAQ,CAAC,MAAMQ,KAAK,CAACE,OAAO,CAAC,cAAc,CAAC,CAAC;IAC7D,MAAMG,SAAQ,GAAIb,QAAQ,CAAC,MAAMQ,KAAK,CAACE,OAAO,CAAC,gBAAgB,CAAC,CAAC;IACjE,MAAMI,eAAc,GAAId,QAAQ,CAC9B,MAAMQ,KAAK,CAACE,OAAO,CAAC,gBAAgB,CACtC,CAAC;IACD,MAAMK,IAAG,GAAIf,QAAQ,CAAC,MAAMQ,KAAK,CAACQ,KAAK,CAACD,IAAI,CAACA,IAAI,CAAC;IAElDd,SAAS,CAAC,MAAM,CAChB,CAAC,CAAC;IAEF,OAAO;MAAEQ,IAAI;MAAEM,IAAI;MAAEH,OAAO;MAAED,QAAQ;MAAEG,eAAe;MAAED;IAAU,CAAC;EACtE;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}